{  
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(js|ts|tsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/test-utils/file-transformer.js"
    },
    "testRegex": "/tests/.*\\.(test|spec)?\\.(ts|tsx)$",
    "transformIgnorePatterns": ["[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"],
    "setupFilesAfterEnv": ["<rootDir>/setup-test.ts"],
    "setupFiles": ["<rootDir>/setup-test.ts"],
    "snapshotSerializers": ["enzyme-to-json"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/test-utils/__mocks__/file-mock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
}